@article{wuttke2008,
  title = {{\textbf{C{\'a}lculo da disponibilidade e da posi{\c c}{\~a}o na curva da banheira de uma v{\'a}lvula de processo petroqu{\'i}mico}}},
  author = {Wuttke, R{\'e}gis Andr{\'e} and Sellitto, Miguel Afonso},
