@book{santos2022,
  title = {{II Filosofia, Vida \& Morte}},
  author = {Santos, Mariana Dias Pinheiro},
  year = {2022},
  month = nov,
  publisher = {Marcos Balieiro},
  address = {Aracaju, SE},
  abstract = {O livro que o leitor tem em m{\~a}os traz parte das apresenta{\c c}{\~o}es realizadas na segunda edi{\c c}{\~a}o do Filosofia, Vida e Morte -- Col{\'o}quio Nacional Online. O evento foi realizado no segundo ano da pandemia de Covid-19 que deixou marcas indel{\'e}veis em tantos de n{\'o}s. De certa forma, o pr{\'o}prio evento se configurou como um espa{\c c}o em que pud{\'e}ssemos debater, {\`a} luz de pensadores consagrados, temas que os desafios enfrentados por todo o mundo tornaram bastante urgentes. Ora, tratava-se de um cen{\'a}rio em que uma amea{\c c}a de escala mundial exigia que repens{\'a}ssemos, a cada instante, que sentido dar{\'i}amos {\`a}s nossas vidas, como poder{\'i}amos nos preparar para lidar com a morte em todas as suas representa{\c c}{\~o}es, que tipo de esfor{\c c}os coletivos (institucionais ou n{\~a}o) poderiam ser empreendidos para lidar com um problema colossal, o que isso significaria para nosso senso de comunidade e, tamb{\'e}m, para a pr{\'o}pria ideia de humanidade. Ao longo de uma semana, temas filos{\'o}ficos que tivessem afinidade com essas quest{\~o}es foram debatidos em alto n{\'i}vel, e de forma bastante apaixonada.  N{\~a}o se deve pensar, entretanto, que a tem{\'a}tica atinente ao evento tem relev{\^a}ncia apenas pelo contexto da pandemia. Como se sabe, o valor da vida, e a melhor maneira de conduzi-la, s{\~a}o temas relevantes para a filosofia desde a Antiguidade cl{\'a}ssica. O mesmo pode ser dito, {\'e} claro, acerca da morte e, tamb{\'e}m, das melhores maneiras de prepara{\c c}{\~a}o para ela. Esses t{\'o}picos ganham import{\^a}ncia j{\'a} com S{\'o}crates e, posteriormente, ser{\~a}o retomados em todas as filosofias que, de algum modo, assimilaram o helenismo. Na modernidade, tamb{\'e}m n{\~a}o foram raros os autores que se esmeraram em tecer considera{\c c}{\~o}es valiosas sobre as melhores maneiras de viver, ou de encarar a morte. J{\'a} na contemporaneidade, viu-se uma diversifica{\c c}{\~a}o, at{\'e} ent{\~a}o inaudita, no que diz respeito a esses temas, que passaram a ser de extrema import{\^a}ncia para reflex{\~o}es existencialistas, para pensadores anal{\'i}ticos que se dedicam a quest{\~o}es como a consci{\^e}ncia e a exist{\^e}ncia de Deus e, ainda, para aqueles que trazem considera{\c c}{\~o}es acerca da vida e da morte para o centro de reflex{\~o}es sobre a pol{\'i}tica, ou mesmo para filosofias menos tradicionais. Todas essas maneiras de pensar sobre o tema tiveram espa{\c c}o no evento, que se caracterizou pela diversidade, pela liberdade e pelo rigor das discuss{\~o}es realizadas.},
  collaborator = {Balieiro, Marcos Ribeiro and de Carvalho, Camila Moura and Ribeiro, Renata Dias and Santos, Allan Wolney Mesquita},
  isbn = {978-65-00-55940-8},
  langid = {brazilian},
  file = {C:\Users\cesar\Dropbox\Apps\Materiais\Atualização_23_1_25 XXXIX 39\Filosofia_2\II_FILOSOFIA_VIDA_E_MORTE.pdf}
}

