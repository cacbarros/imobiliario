@article{bevilaqua2019,
  title = {{An{\'a}lise da ventila{\c c}{\~a}o natural: simula{\c c}{\~o}es CFD e ensaios em um modelo f{\'i}sico reduzido}},
  author = {Bevilaqua, Camila Pizaia and Bressianini, Bianca and Azuma, Mauricio Hidemi and Lukiantchuki, Marieli Azoia},
  year = {2019},
  journal = {PARC Pesquisa em Arquitetura e Constru{\c c}{\~a}o},
  volume = {10},
  pages = {e019022-e019022},
  issn = {1980-6809},
  doi = {10.20396/parc.v10i0.8652835},
  urldate = {2025-03-16},
  abstract = {Apesar das in{\'u}meras vantagens da integra{\c c}{\~a}o dos conceitos bioclim{\'a}ticos ao projeto arquitet{\^o}nico, estudos demonstram que para a maioria dos arquitetos existe uma dificuldade em sua real aplica{\c c}{\~a}o. Isso indica a exist{\^e}ncia de uma lacuna entre o conhecimento acad{\^e}mico e a pr{\'a}tica de projeto, principalmente no que se refere {\`a} ventila{\c c}{\~a}o natural. Muitas das estrat{\'e}gias de ventila{\c c}{\~a}o natural s{\~a}o apresentadas por meio de desenhos vision{\'a}rios que n{\~a}o representam claramente o seu desempenho nos ambientes internos. O objetivo dessa pesquisa {\'e} comprovar a materializa{\c c}{\~a}o de princ{\'i}pios b{\'a}sicos de ventila{\c c}{\~a}o natural por meio de um m{\'e}todo simplificado, visando facilitar o entendimento desses fen{\^o}menos pelos alunos durante o ensino da ventila{\c c}{\~a}o natural e, posteriormente, sua aplica{\c c}{\~a}o no projeto arquitet{\^o}nico. O estudo comparativo dividiu-se em tr{\^e}s etapas a fim de comprovar a efici{\^e}ncia do modelo proposto para uso no processo de ensino: (i) constru{\c c}{\~a}o de um modelo f{\'i}sico flex{\'i}vel e simplificado para uso no ensino da ventila{\c c}{\~a}o natural; (ii) ensaios experimentais no modelo proposto, com rela{\c c}{\~a}o ao impacto da localiza{\c c}{\~a}o das aberturas no desempenho do fluxo de ar interno nas edifica{\c c}{\~o}es; (iii) simula{\c c}{\~o}es computacionais com base na Din{\^a}mica dos Flu{\'i}dos Computacional (CFD) dos mesmos cen{\'a}rios avaliados pelo m{\'e}todo simplificado. A compara{\c c}{\~a}o dos resultados experimentais e num{\'e}ricos demonstrou uma compatibilidade entre as duas ferramentas utilizadas, mostrando que {\'e} poss{\'i}vel o uso desse modelo simplificado para o ensino de princ{\'i}pios b{\'a}sicos da ventila{\c c}{\~a}o natural como, por exemplo, a defini{\c c}{\~a}o da localiza{\c c}{\~a}o das aberturas.},
  copyright = {https://creativecommons.org/licenses/by/4.0},
  langid = {portuguese},
  keywords = {Qualidade do Ar e Ventilacao,Simulacao energetica},
  file = {C:\Users\cesar\Dropbox\Apps\Materiais\Atualização 28_1_24 IX 10\Engenharia\Analise_da_ventilacao_natural.pdf}
