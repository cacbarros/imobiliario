@article{varela2023,
  title = {{La Inteligencia artificial y el futuro de la educaci{\'o}n superior: : desaf{\'i}os y oportunidades}},
  shorttitle = {{La Inteligencia artificial y el futuro de la educaci{\'o}n superior}},
  author = {Varela, Yessika Zamora and Encinas, Maria del Carmen Mendoza},
  year = {2023},
  month = aug,
  journal = {Horizontes pedag{\'o}gicos},
  volume = {25},
  number = {1},
  pages = {1--13},
  issn = {2500-705X},
  doi = {10.33881/0123-8264.hop.25101},
  urldate = {2024-03-10},
  abstract = {The development of new technologies has permeated all areas, with education being one of the least exposed to them. Over the past two decades, institutions have invested money, efforts and personnel in their implementation in order to prepare students for its use and application. It is in this context that Artificial Intelligence becomes very important. This had its beginnings in the fifties; however, it is not until the beginning of this century that development companies have begun to offer their products to the general public. This paper reflects on Artificial Intelligence and its role in higher education in Latin America, making an approach to the challenges it faces and the opportunities it poses, from a documentary point of view. Likewise, it makes pertinent recommendations for the methodological approach to the use of Artificial Intelligence in the educational framework at this level. Through the application of a survey as an instrument for measuring knowledge, approach and opinion on Artificial Intelligence, it carries out the corresponding analysis in order to be able to adequately develop the approaches that have been mentioned. The instrument includes 20 questions including multiple choice and open-ended answer that analyze knowledge, types of use, perception of ease, usefulness, efficiency and effectiveness of the tools or systems of Artificial Intelligence used. The results demonstrate the use and application of a significant percentage of the population. At the same time, they present the need to develop clear guidelines for their use in the educational field and a clear need to clarify related ethical concepts Finally, recommendations applicable to the Strategy on Technological Innovation in Education within the Beijing Consensus on Artificial Intelligence and Education are presented. These include the main aspects related to the role of the teacher, regulations related to copyright, human, technological and training resources requirements and inclusion in the higher-level curriculum.},
  copyright = {Derechos de autor 2023 Horizontes pedag{\'o}gicos},
  langid = {spanish},
