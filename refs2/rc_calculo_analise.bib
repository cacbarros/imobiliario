@article{HistoriaMatematica2024,
  title = {{Hist{\'o}ria da matem{\'a}tica}},
  year = {2024},
  month = nov,
  journal = {Wikip{\'e}dia, a enciclop{\'e}dia livre},
  urldate = {2025-01-02},
  abstract = {A hist{\'o}ria da matem{\'a}tica trata da origem das descobertas matem{\'a}ticas e dos m{\'e}todos e nota{\c c}{\~o}es matem{\'a}ticas do passado. Antes da era moderna e da dissemina{\c c}{\~a}o mundial do conhecimento, exemplos escritos de novos desenvolvimentos matem{\'a}ticos vieram {\`a} luz apenas em alguns locais. A partir de 3000 a.C., os estados mesopot{\^a}micos da Sum{\'e}ria, Ac{\'a}dia e Ass{\'i}ria, seguidos de perto pelo Egito Antigo e o estado levantino de Ebla come{\c c}aram a usar aritm{\'e}tica, {\'a}lgebra e geometria para fins de tributa{\c c}{\~a}o, com{\'e}rcio, trocas e tamb{\'e}m nos padr{\~o}es da natureza, no campo da astronomia e para registrar o tempo e formular calend{\'a}rios. Os primeiros textos matem{\'a}ticos dispon{\'i}veis s{\~a}o da Mesopot{\^a}mia e Egito -- Plimpton 322 (Babil{\^o}nia c. 2000 -- 1900 a.C.), o Papiro de Rhind (Eg{\'i}pcio c. 1800 a.C.) e o Papiro de Moscou (Eg{\'i}pcio c. 1890 a.C.). Todos esses textos mencionam os chamados ternos pitag{\'o}ricos, portanto, por infer{\^e}ncia, o teorema de Pit{\'a}goras parece ser o desenvolvimento matem{\'a}tico mais antigo e difundido depois da aritm{\'e}tica e da geometria b{\'a}sicas. O estudo da matem{\'a}tica como uma "disciplina demonstrativa" come{\c c}ou no s{\'e}culo VI a.C. com os pitag{\'o}ricos, que cunharam o termo "matem{\'a}tica" do grego antigo {$\mu\acute{\alpha}\theta\eta\mu\alpha$} (matema), que significa "sujeito de instru{\c c}{\~a}o". A matem{\'a}tica grega refinou muito os m{\'e}todos (especialmente atrav{\'e}s da introdu{\c c}{\~a}o do racioc{\'i}nio dedutivo e do rigor matem{\'a}tico nas provas) e expandiu o assunto da matem{\'a}tica. Embora n{\~a}o tenham feito virtualmente nenhuma contribui{\c c}{\~a}o para a matem{\'a}tica te{\'o}rica, os antigos romanos usavam a matem{\'a}tica aplicada no levantamento topogr{\'a}fico, engenharia estrutural, engenharia mec{\^a}nica, contabilidade, cria{\c c}{\~a}o de calend{\'a}rios lunares e solares, e at{\'e} artes e of{\'i}cios. A matem{\'a}tica chinesa fez contribui{\c c}{\~o}es iniciais, incluindo um sistema de valor posicional e o primeiro uso de n{\'u}meros negativos. O sistema num{\'e}rico hindu-ar{\'a}bico e as regras para o uso de suas opera{\c c}{\~o}es, em uso em todo o mundo hoje, evolu{\'i}ram ao longo do primeiro mil{\^e}nio d.C. na {\'I}ndia e foram transmitidos para o mundo ocidental via matem{\'a}tica isl{\^a}mica atrav{\'e}s do trabalho de Alcuarismi. A matem{\'a}tica isl{\^a}mica, por sua vez, desenvolveu e expandiu a matem{\'a}tica conhecida por essas civiliza{\c c}{\~o}es. Contempor{\^a}nea com, mas independente dessas tradi{\c c}{\~o}es, foi a matem{\'a}tica desenvolvida pela civiliza{\c c}{\~a}o maia do M{\'e}xico e da Am{\'e}rica Central, onde o conceito de zero recebeu um s{\'i}mbolo padr{\~a}o em numerais maias. Muitos textos gregos e {\'a}rabes sobre matem{\'a}tica foram traduzidos para o latim a partir do s{\'e}culo XII, levando a um maior desenvolvimento da matem{\'a}tica na Europa Medieval. Desde os tempos antigos at{\'e} a Idade M{\'e}dia, os per{\'i}odos de descoberta matem{\'a}tica foram frequentemente seguidos por s{\'e}culos de estagna{\c c}{\~a}o. Come{\c c}ando na It{\'a}lia renascentista no s{\'e}culo XV, novos desenvolvimentos matem{\'a}ticos, interagindo com novas descobertas cient{\'i}ficas, foram feitos em um ritmo crescente que continua at{\'e} os dias atuais. Isso inclui o trabalho inovador de Isaac Newton e Gottfried Wilhelm Leibniz no desenvolvimento do c{\'a}lculo infinitesimal ao longo do s{\'e}culo XVII.},
  copyright = {Creative Commons Attribution-ShareAlike License},
  langid = {portuguese},
  annotation = {Page Version ID: 69013659},
  file = {C:\Users\cesar\Zotero\storage\SVVE2AM3\História_da_matemática.html}
}

